--update and draw--
function _update60()
		player_update()
		player_animate()
		update_bats()
		update_msg()
			
		--double camera
		cam_x=player.x-64+(player.w/2)
		cam_x = mid(map_start_x,cam_x,map_end_x-128)
	
	 cam_y=player.y-64+(player.h/2)
		cam_y = mid(map_start_y,cam_y,map_end_y-128)
	
		camera(cam_x,cam_y)
		
		--animations--
		if sp_anim<2.9 then
		sp_anim=sp_anim+.05
		else
		sp_anim=1
	end
end

function _draw()
	cls()
	palt(15,true)
	palt(0,false)
	map()
	--bats
	for b in all(bat) do
		spr(frm_batg[flr(sp_anim)],b.x,b.y,1,1)
		spr(frm_batb[flr(sp_anim)],b.x+4,b.y+4,1,1)
	
	end
	--animations
	anim_items()
	
	--player
		spr(player.sp,player.x,player.y,1,1,player.flp)
	draw_msg(player)

end

