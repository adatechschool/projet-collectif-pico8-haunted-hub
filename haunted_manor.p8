function create_msg(name,...)
	msg_title=name
	messages={...}
end

function update_msg() --met a jour l'index du tableau de messages
	if (btnp(🅾️)) then --designe n sur le clavier
		deli(messages,1) --supprime petit a petit les indexs du tabelau de messages
	end 
end
	
function draw_msg(obj) -- parametres d'affichage du message
	if messages[1] then
		local y=(obj.y-45) -- associe a l'axe y une valeur locale
		local x=(obj.x-60)-- associe a l'axe x une valeur locale
		rectfill(x,y,
		x+#msg_title*4,y+5,0) -- place un rectangle derriere msg_title
		print(msg_title,x,y,11) -- decide ou est place le titre
		
		rectfill(x,y+9,x+130,y+30,0) -- place un rectangle derriere msg_title
		print(messages[1],x,y+10,9) -- decide ou est place le msg
	end
end

